{
    "manifest_version": 3,
    "name": "TEST",
    "description": "TEST",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "TEST"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "externally_connectable": {
        "matches": ["*://*/*"]
    }
}